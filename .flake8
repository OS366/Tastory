[flake8]
max-line-length = 120
ignore = E203, W503, E501, E722
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    build,
    dist,
    .eggs,
    *.egg,
    frontend,
    node_modules
max-complexity = 90
per-file-ignores =
    __init__.py:F401
    tests/*.py:F401,F811
    app.py:C901,F401,F841,F824 